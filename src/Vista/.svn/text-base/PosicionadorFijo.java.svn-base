package Vista;

import ar.uba.fi.algo3.titiritero.Posicionable;

public class PosicionadorFijo implements Posicionable{

	private int x;
	private int y;
	
	public PosicionadorFijo(int x,int y){
		this.x = x;
		this.y = y;
	}
	
	@Override
	public int getX() {
		return x;
	}

	@Override
	public int getY() {
		return y;
	}

}
